package br.usp.ime.choreos;

import java.util.List;

public class EnvelopeArgumentGenerator {

	public static String generate(String xml, List<String> parameters){

		for(int i = 0; i < parameters.size(); i++){
			
			// Regex for the current argument
			String argRegex = "<arg" + i + ">?";
			xml.replaceFirst("", "<arg" + i + ">" + parameters.get(i));
		}
			
		return null;
	}
}
