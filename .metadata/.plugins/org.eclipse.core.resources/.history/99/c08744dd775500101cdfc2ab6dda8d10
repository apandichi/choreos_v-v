package br.usp.ime.choreos.vvws.ws;

import java.util.List;

import br.usp.ime.choreos.vvws.model.CD;

public class MockCDs {

	private static List<CD> mockList;
	
	static {
		mockList.add(new CD("The dark side of the moon", "Pink Floyd", "Psychodelic Rock", 9));
		mockList.add(new CD("Led Zeppelin I", "Led Zeppelin", "Hard Rock", 8));
		mockList.add(new CD("Have a nice day", "Bon Jovi", "Rock", 10));
		mockList.add(new CD("Help!", "The Beatles", "Rock", 7));
		mockList.add(new CD("Sevilla Barbershop", "Rossini", "Opera", 4));
		mockList.add(new CD("Notcurnes", "Frederic Chopin", "Classical", 15));
		mockList.add(new CD("Black Album", "Metallica", "Metal", 12));
	}
	
	public static List<CD> generateMockCDs(){
		return mockList;
	}

}
